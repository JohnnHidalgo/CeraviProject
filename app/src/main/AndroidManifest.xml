<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.johnnhidalgo.ceraviproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Estadisticas"
            android:label="@string/title_activity_estadisticas"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Moldeado"
            android:label="@string/title_activity_moldeado"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Arcilla"
            android:label="@string/title_activity_arcilla"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Cargue"
            android:label="@string/title_activity_cargue"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Quemado"
            android:label="@string/title_activity_quemado"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Descargue"
            android:label="@string/title_activity_descargue"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MenuClienteListActivity"
            android:label="@string/title_menucliente_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MenuClienteDetailActivity"
            android:label="@string/title_menucliente_detail"
            android:parentActivityName=".MenuClienteListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.johnnhidalgo.ceraviproject.MenuClienteListActivity" />
        </activity>
        <activity
            android:name=".AgregarClienteListActivity"
            android:label="@string/title_agregarcliente_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AgregarClienteDetailActivity"
            android:label="@string/title_agregarcliente_detail"
            android:parentActivityName=".AgregarClienteListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.johnnhidalgo.ceraviproject.AgregarClienteListActivity" />
        </activity>
        <activity
            android:name=".MenuObreroListActivity"
            android:label="@string/title_menuobrero_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MenuObreroDetailActivity"
            android:label="@string/title_menuobrero_detail"
            android:parentActivityName=".MenuObreroListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.johnnhidalgo.ceraviproject.MenuObreroListActivity" />
        </activity>
        <activity
            android:name=".AgregarObreroListActivity"
            android:label="@string/title_agregarobrero_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".AgregarObreroDetailActivity"
            android:label="@string/title_agregarobrero_detail"
            android:parentActivityName=".AgregarObreroListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.johnnhidalgo.ceraviproject.AgregarObreroListActivity" />
        </activity>
    </application>

</manifest>